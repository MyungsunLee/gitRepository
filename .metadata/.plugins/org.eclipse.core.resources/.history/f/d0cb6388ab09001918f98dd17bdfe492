
public class FlowEx6Test4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n = 0;
		int add = 0;
		n = 2;
		add = 1;
		while (add < 10) 
		System.out.println("2´Ü");{
			
//			System.out.println(n + "´Ü");
			System.out.println(n + "*" + add + " = " + (n*add));
			add++;
			
		}


	}
}
