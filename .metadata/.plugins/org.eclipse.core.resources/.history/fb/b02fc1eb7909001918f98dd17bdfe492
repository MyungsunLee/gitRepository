import com.sun.org.apache.xpath.internal.Arg;

public class ForFlowEx4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//ÁßÃ¸for¹®
		
		
		for (int i = 1; i <=9 ; i++) {
			
			for (int n = 2; n <=9 ; n++) {
			System.out.println(i + "*" + n + "=" + (n * i));
		}
			System.out.println();
	}

	}}
