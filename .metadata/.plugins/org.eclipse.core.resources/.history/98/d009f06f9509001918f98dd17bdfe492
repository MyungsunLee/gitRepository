
public class ForDuplicate {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		String bnk = " ";
		for(int i = 0; i <5; i++) {

			for (int n = 0; n <= i ;n++) {
				System.out.print("*");
				for(m = 5; n<=i; m--) {
					System.out.println(bnk);
				}
		}
			
			System.out.println();

	}

}
}