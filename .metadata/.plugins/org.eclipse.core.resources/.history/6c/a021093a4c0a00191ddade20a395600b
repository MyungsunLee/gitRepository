
public class OneToOneHundred2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num = 0;
		num = 1;
		int sum = 0;

		
		for (num = 1;num <= 100 ; num++ ) {
			sum = (num + 1);
			System.out.println(num  + " + " + sum +  " = " + (num + sum));
			
		}
		
		
		
	}

}
