
public class ArrayEx5Test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int [] num5 = new int[5];
		int [] num10 = new int[10];
		
		num5[0] = 2;
		num5[0] = num10[0];
		
		num5[1] = 20;		
		num5[1] = num10[1];
		
		num5[2] = 12;
		num5[2] = num10[2];
		
		num5[4] = 2;
//									5~9 배열넘버와 같은 값
		
		int i = 0;
		for(i = 3; i < num10.length; i++) {
			num10[i] = i;
			System.out.println("num10 [" + i + "]의 값 : " + num10[i]);
		}
		
		
		
	}

}
