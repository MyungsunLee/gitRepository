
public class ArrayEx9 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] lottery = new int[45];
		
		for (int i = 0; i < lottery.length; i++) {
			lottery[i] = i+1;
		}
		for (int i = 0; i < lottery.length; i++) {
			System.out.println(lottery[i] + " ");
		}
	
	}

}
