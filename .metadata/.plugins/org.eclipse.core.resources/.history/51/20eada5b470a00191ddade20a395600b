import java.util.Scanner;

public class BmiCarculatingEx1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner scan = new Scanner(System.in);
		
		double weight = 0.0;
		double high = 0.0;
		double bmi = 0;
		
		System.out.println("몸무게를 입력해 주세요");
		weight = scan.nextInt();
		
		System.out.println("키를 입력해 주세요");
		high = scan.nextInt();
		
		bmi = weight/((high*high)/1000);
		System.out.println(bmi);
		if (bmi < 19) {
			System.out.println("bmi : " + bmi );
			System.out.println("검사결과 저체중 입니다");
		}else if (bmi < 25) {
				System.out.println("bmi : " + bmi );
				System.out.println("검사결과 정상체중 입니다");
			}else if (bmi > 25) {
					System.out.println("bmi : " + bmi);
					System.out.println("검사결과 과체중 입니다");
				}
			}
		
	}


