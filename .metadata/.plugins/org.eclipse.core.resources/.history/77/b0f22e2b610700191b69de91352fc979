
public class TriangleOperEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x = 0;
		int y = -10;
		
		x = 10;
		y = -10;
		
		int absX = (x >= 0) ? x : -x;
		int absY = (y >= 0) ? y : -y;
		
		System.out.println("x = 10일 때, x의 절대값은 " + absX);
		System.out.println("y = -10일 때, y의 절대값은 " + absY);
		
		
		
		
		
		
	}

}
