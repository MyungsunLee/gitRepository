
public class ArrayEx5Test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int [] num5Arr = new int[5];
		int [] num10Arr = new int[10];
		
		num5Arr[0] = 2;
		num5Arr[0] = num10Arr[0];
		
		num5Arr[1] = 20;		
		num5Arr[1] = num10Arr[1];
		
		num5Arr[2] = 12;
		num5Arr[2] = num10Arr[2];
		
		num5Arr[4] = 2;
//									5~9 배열넘버와 같은 값
		int ii = 0;
		for(ii = 0; ii < num5Arr.length ; ii++) {
			System.out.println("num5Arr [" + ii + "]의 값 : " + num5Arr[ii]);
		}
		
		int i = 0;
		for(i = 5; i < num10Arr.length; i++) {
			num10Arr[i] = i;
			System.out.println("num10Arr [" + i + "]의 값 : " + num10Arr[i]);
		}
		
		
		
	}

}
