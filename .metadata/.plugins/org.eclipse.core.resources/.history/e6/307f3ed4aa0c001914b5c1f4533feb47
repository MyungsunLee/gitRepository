
public class ArrayEx12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String[] nameArr = new String[3];
		
		nameArr[0] = "txt1";
		nameArr[1] = "txt2";
		nameArr[2] = "txt3";

		for (int i = 0; i < nameArr.length; i++) {
			System.out.println(nameArr[i]);
		}
		
		
		
		
		
		
	
	}

}
