
public class ForFlowEx4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//ÁßÃ¸for¹®
		
		
		for (int i = 2; i <=9 ; i++) {
			
			for (int n = 1; n <= 9 ; n++) {
			System.out.println();
			System.out.print(i + " * " + n + " = " + (n * i) + "\t");
		}
			System.out.println();
	}

	}}
