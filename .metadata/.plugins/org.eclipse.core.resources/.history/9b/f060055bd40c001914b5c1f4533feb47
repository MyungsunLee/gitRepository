
public class PreTime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//비객체지향				객체지향
		//한국시간
		int hour = 0;
		int minute = 0;
		double second = 0;
		
//		미국시간
		int aHour = 0;
		int aMinute = 0;
		double aSecond = 0;
		
		
		hour = 5;
		minute = 8;
		second = 21;
		
		
		
		aHour = 3;
		aMinute = 8;
		aSecond = 21;
		System.out.println("한국시간 : 시" + hour + " 분 " + minute + " 초" + second);
		System.out.println("미국시간 : 시" + aHour + " 분 " + aMinute + " 초" + aSecond);
		
	}

}
