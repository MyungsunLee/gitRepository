
public class OneToOneHundred2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num = 0;
		num = 1;
		int sum = 0;
		sum = num + (num + 1);
		
		for (num = 0;num <= 10 ; num++ ) {
			System.out.println(num  + " + " + sum +  " = " + (num + sum));
			
		}
		
		
		
	}

}
