
public class FlowEx2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int score = 0;
		score = 60;
		
		if(score > 60) {
			System.out.println("합격입니다");
			System.out.println("축하합니다");
		}
		if (score ==60) {
			System.out.println("아슬아슬 하셨네요");
		}
		if (score <60) {
			System.out.println("불합격입니다");
			
		}
	}
}
