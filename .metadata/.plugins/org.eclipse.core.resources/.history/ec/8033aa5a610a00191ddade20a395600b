
public class ArrayEx4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int[]numArr  = new int[5];
		int[]backUpNumArr = new int[5];
		
		int num = 0;
		int backNumArr = 0;
		
		num = 1;
		backNumArr = num;
		System.out.println("원본 데이터 : " + num);
		System.out.println("백업 데이터 : " + backNumArr);
		
		numArr[0] = 10;
		backUpNumArr[0] = 10;
		System.out.println("원본 데이터 : " + numArr[0]);
		System.out.println("백업 데이터 : " + backUpNumArr[0]);
		
		
		
		
		}
		

}
