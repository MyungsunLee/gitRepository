
public class ArrayEx4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] numArr = new int[5]; // 0 1 2 3 4
		int[] backUpNumArr = new int[5]; // 0 1 2 3 4

		int num = 0;
		int backUpNum = 0;

		num = 1;
		backUpNum = num;
		System.out.println("원본 데이터 : " + num);
		System.out.println("원본 데이터 : " + backUpNum);

		System.out.println("=================================");
		for (int i = 0; i < numArr.length; i++) {
			System.out.println("백업 데이터 : " + backUpNumArr[i]);
		}

		numArr[0] = 10;
		backUpNumArr[0] = 10;
		System.out.println("=================================");
		for (int i = 0; i < backUpNumArr.length; i++) {
			System.out.println("원본 데이터 : " + numArr[0]);
			System.out.println("백업 데이터 : " + backUpNumArr[0]);

		}

	}

}
