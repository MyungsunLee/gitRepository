import java.util.Scanner;

public class InputEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner scan = new Scanner(System.in);
		int num = 0;
		scan.nextInt();
		
		System.out.println("숫자를입력해주세요");
		num = scan.nextInt();
		
		System.out.println(num);
		
	}

}
